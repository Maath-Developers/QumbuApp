@using Microsoft.AspNetCore.Identity
@using Qumbu_Community_Health_Care_Center.Areas.Identity.Data

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Pluto - Responsive Bootstrap Admin Panel Templates</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- site icon -->
    <link rel="stylesheet" href="~/css/StyleSheet/Workshop_StyleSheet.css">

    <link rel="icon" href="~/_Images/favicon.ico" type="image/png" />
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/_css/bootstrap.min.css" />
    <!-- site css -->
    <link rel="stylesheet" href="~/_css/style.css" />
    <!-- responsive css -->
    <link rel="stylesheet" href="~/_css/responsive.css" />
    <!-- color css -->
    <link rel="stylesheet" href="~/_css/colors.css" />
    <!-- select bootstrap -->
    <link rel="stylesheet" href="~/_css/bootstrap-select.css" />
    <!-- scrollbar css -->
    <link rel="stylesheet" href="~/_css/perfect-scrollbar.css" />
    <!-- custom css -->
    <link rel="stylesheet" href="~/_css/custom.css" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style>
        /* Increase the height of the navigation bar */
        .navbar {
            min-height: 60px; /* Adjust the value as needed */
        }

       
    </style>

</head>
<body>

    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            <div class="wrap">
                <nav id="sidebar" class="justify-content-cente">
                    <div class="logo_sectiont">
                        <a href="index.html"><img class="img-responsive" src="~/_Images/1.png" alt="#" /></a>
                    </div>
                    <hr />
                    <div class="sidebar-content fa-align-center">
                        <div class="sidebar_blog_2 justify-content-center">

                            <ul class="list-unstyled components h4">
                                <br>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Admin_Dashboard">Dashboard</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Create_File">Create file</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Appointment_Booking">Appointment</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Patient_Reg">Patient Registration</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Create_File">Manage Queue</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Admin" asp-action="Create_File">Doctor Availability</a>
                                    </button>
                                </li>
                                <li class="justify-content-center text-center">
                                    <button class="w-75 mb-3 text-center">
                                        <a class="list-group-item bg bg-dark font-weight-bold text-center" asp-controller="Home" asp-action="Index">Home</a>
                                    </button>
                                </li>

                            </ul>
                        </div>

                    </div>
                </nav>
            </div>

            <!-- end sidebar -->
            <!-- right content -->
            <div id="content">
                <!-- topbar -->
                <div class="topbar">
                    <nav class="navbar navbar-expand-lg ">
                        <div class="full">

                            <div class="right_topbar">
                                <div >
                                    
                                    <ul class="user_profile_dd">
                                        <li>
                                            <div class="order-last nav-item btn-outline-success ml-auto">
                                                <partial name="_LoginPartial" />
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
                <!-- end topbar -->
                <!-- dashboard inner -->
                <div class="midde_cont">
                    <br />
                    <br />
                    <br />
                    <br />
                    @RenderBody()
                    <!-- footer -->

                </div>
                <!-- end dashboard inner -->
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/_js/jquery.min.js"></script>
    <script src="~/_js/popper.min.js"></script>
    <script src="~/_js/bootstrap.min.js"></script>
    <!-- wow animation -->
    <script src="~/_js/animate.js"></script>
    <!-- select country -->
    <script src="~/_js/bootstrap-select.js"></script>
    <!-- owl carousel -->
    <script src="~/_js/owl.carousel.js"></script>
    <!-- chart js -->
    <script src="~/_js/Chart.min.js"></script>
    <script src="~/_js/Chart.bundle.min.js"></script>
    <script src="~/_js/utils.js"></script>
    <script src="~/_js/analyser.js"></script>
    <!-- nice scrollbar -->
    <script src="~/_js/perfect-scrollbar.min.js"></script>
    <script>
        var ps = new PerfectScrollbar('#sidebar');
    </script>
    <!-- custom js -->
    <script src="~/_js/custom.js"></script>
    <script src="~/_js/chart_custom_style1.js"></script>
</body>
</html>



