@model Qumbu_Community_Health_Care_Center.Models.FamilyReg;
@{
	ViewData["Title"] = "CreateFamilyReg";
	Layout = "~/Views/Shared/_FamilyPlanLayout.cshtml";
}

<div class="row bg-white border border-3 border-info p-3 col-12">
	<div class="form-group row">
		<h2 class="text-black-50 pe-lg-5 text-center">Let's have a chat!! <a href="index.html"><img class="img-responsive" width="100" height="100" src="~/_Images/FamilyReg.png" alt="#" /></a></h2>
	</div>
	<br />
	<form method="post" asp-action="CreateFamilyReg">

		<div asp-validation-summary="ModelOnly" class="text-danger"></div>
		
		<div class=" row container mt-8">
			<div class="card">
				<div class="card-header bg-secondary-emphasis">
					<h5 class="card-title">Peronal Details</h5>
				</div>
				<div class="card-body">
					<div class="row">

						<div class="col-md-3">
							<div class="form-group">
								<label for="memberName">ID Number</label>
								<input type="text" class="form-control" id="memberName" placeholder=" ">
							</div>
						</div>
						<div class="col-md-3">
							<div class="form-group">
								<label for="memberName">Email Address</label>
								<input type="text" class="form-control" id="memberName" placeholder=" ">
							</div>
						</div>
						<div class="col-md-3">
							<div class="form-group">
								<label for="memberName">Cell</label>
								<input type="text" class="form-control" id="memberName" placeholder=" ">
							</div>
						</div>
						<div class="col-md-3">
							<div class="form-group">
								<label for="memberName">First Name</label>
								<input type="text" class="form-control" id="memberName" placeholder=" ">
							</div>
						</div>
						<div class="col-md-3">
							<div class="form-group">
								<label for="memberName">Last Name</label>
								<input type="text" class="form-control" id="memberName" placeholder=" ">
							</div>
						</div>
					

						<div class="col-md-3">
							<div class="form-group">
								<label for="dateOfBirth">Date</label>
								<input type="date" class="form-control" id="dateOfBirth">
							</div>
						</div>
						<div class="col-md-3">
							<div class="form-group">
								<label for="dateOfBirth">D.O.B</label>
								<input type="date" class="form-control" id="dateOfBirth">
							</div>
						</div>
					

					</div>

				</div>
			</div>

		</div>

			<div class="row">
			   <div class="form-group col-4 ">
				<label asp-for="intercourse"></label>
				<select id="Dropdown" asp-for="intercourse" class="form-control" itemid="validationtype" required>
					<option value="Days">2/3 days back</option>
					<option value="1 week">A Week</option>
					<option value="2 weeks">2 Weeks back</option>
					<option value="Month">Been Month</option>

				</select>
				<span asp-validation-for="child" class="text-danger"></span>
			 </div>
			   <div class="form-group col-3">
				<label asp-for="period"></label>
				<input asp-for="period" class="form-control" />
				<span asp-validation-for="period" class="text-danger"></span>
			 </div>
			</div>
			<div class="row">
			   <div class="form-group col-4">
				<label asp-for="child"></label>
				<select id="Dropdown" asp-for="child" class="form-control" itemid="validationtype" required>
					<option value="Yes">Yes</option>
					<option value="No">No</option>
				</select>
				<span asp-validation-for="child" class="text-danger"></span>
			 </div>
			   <div class="form-group col-3">
				<label asp-for=" experience"></label>
				<select id="Dropdown" asp-for="experience" class="form-control" itemid="validationtype" required>
					<option value="Yes">Yes</option>
					<option value="No">No</option>
				</select>
				<span asp-validation-for=" experience" class="text-danger"></span>
			</div>
			</div>
			<div class="row">
			    <div class="form-group col-4">
				<label asp-for=" allaegies"></label>
				<select id="Dropdown" asp-for="allaegies" class="form-control" itemid="validationtype" required>
					<option value="Yes">Yes</option>
					<option value="No">No</option>
				</select>
				<span asp-validation-for="allaegies" class="text-danger"></span>
			  </div>
			    <div class="form-group col-3">
				<label asp-for="medication"></label>
				<select id="Dropdown" asp-for="medication" class="form-control" itemid="validationtype" required>
					<option value="Yes">Yes</option>
					<option value="No">No</option>
				</select>
				<span asp-validation-for="medication" class="text-danger"></span>
			  </div>
			</div>
			<div class="row">

			    <div class="form-group col-4">
				<label asp-for="contraceptives"></label>
				<select id="Dropdown" asp-for="contraceptives" class="form-control" itemid="validationtype" required>
					<option value="Yes">Yes</option>
					<option value="No">No</option>
				</select>
				<span asp-validation-for="contraceptives" class="text-danger"></span>
			  </div>
			    <div class="form-group col-3">
				<label asp-for="type"></label>
				<select id="dropdown" asp-for="type" class="form-control" itemid="validationtype" required>
					<option value="Pills">Pills</option>
					<option value="Injection">Injection</option>
					<option value="Loop">Loop</option>
					<option value="Implant">Implant</option>
					<option value="Condoms">Condoms</option>
					<option value="Vigina Ring">Vigina Ring</option>
				</select>
				<span asp-validation-for="type" class="text-danger"></span>
			  </div>
			</div>
			<br />
			<br />
			<div class="form-group row">
				<div class="col-8 offset-2 row">
							<div class="col">
								<input type="submit" class="btn btn-outline-info w-25" value="Submit" />
							</div>
							<div class="col">
								<a asp-action="ViewFamilyReg" class="btn btn-outline-info w-25">View</a>
							</div>
						</div>
		    </div>
			<br />
			<br />
			
		

	</form>
</div>


